{
  "extends": "eslint:recommended",
  "plugins": [
    "angular"
  ],
  "rules": {
    "angular/controller-as": 0,
    "angular/controller-as-vm": 0
  },
  "env": {
    "es6": true,
    "browser": true,
    "jasmine": true
  },
  "globals": {
    "angular": true,
    "module": true,
    "inject": true,
    "moment": true,
    "environment": true
  },
  "parser": "babel-eslint"
}